    <script autostart="false" src="_framework/blazor.server.js"></script>
    <script>
        Blazor.start({
            reconnectionHandler: {
                onConnectionDown: (options, error) => {
                    new Promise(
                        () => setTimeout(
                            () => {
                                Blazor.reconnect();
                                location.reload();
                            }, (1000 * 10)
                        )
                    );
                },
                onConnectionUp: () => console.log("Up, up, and away!")
            }
        });

    </script>
